class Solution {
public:
    vector<int> minOperations(string boxes) {
        vector<int> arr;
        int len = boxes.size();
        for(int i=0;i<len;i++) {
            int sum=0;
            for(int j=0;j<len;j++) {
                if(boxes[j] == '1') {
                    sum = sum + abs(j-i);
                }
            }
            arr.push_back(sum);
        }
        return arr;
    }
};