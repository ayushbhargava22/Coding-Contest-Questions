class Solution {
public:
    vector<int> diStringMatch(string s) {
        vector<int> arr;
        int n=s.size();
        int j=0;
        int k=n;
        for(int i=0;i<n;i++) {
            if(s[i]=='I') {
                arr.push_back(j);
                j++;
            }
            else {
                arr.push_back(k);
                k--;
            }
        }
        arr.push_back(k);
        return arr;
    }
};